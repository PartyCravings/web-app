							{% if file is not null and file.mimetype matches '/^image.*/' %}
								<div class="image"><img class="lazy" data-u="image" alt="{{ file.originalName }}" src="data:image/svg+xml;base64, {{ file.silhouette|base64encode }}" data-src="{{ asset(storage_link  ~ '/' ~ file.name) }}"/></div>
							{% elseif file is not null and file.mimetype matches '/^video.*/' %}
